{
  // 配件重量，单位 kg。影响持枪者的移速
  "weight": 0,
  // 对瞄准时间的影响，加数，单位为秒，可为负数
  "ads_addend": 0.1,
  "recoil_modifier": {
    // 竖直方向偏转削减的大小，范围-1~0
    "pitch": -0.34,
    // 水平方向偏转削减程度的大小，范围-1~0
    "yaw": -0.15
  },
  "inaccuracy": {
    "percent": -0.95
  },
  "melee": {
    // 刺刀距离参数，枪械里还有个刺刀攻击距离参数，两者做加和
    "distance": 3,
    // 刺刀范围角度
    "range_angle": 45,
    // 刺刀冷却时间，枪械里还有个刺刀冷却时间参数，两者做加和
    "cooldown": 0,
    // 伤害值
    "damage": 10,
    // 击退效果
    "knockback": 1,
    // 前摇时长，单位：秒
    "prep": 0.2
  }
}